/*To find the sum of a array using function*/
#include<stdio.h>
int arraysum(int a[])//Function defination
{
    int i,sum=0;
    for(i=0;i<5;i++)
        sum=sum+a[i];
    return sum;
}

int main()//function calling
{
    int a[5],i,sum;
    printf("Enter the 5 element:");
    for(i=0;i<5;i++)
        scanf("%d",&a[i]);
    sum=arraysum(a);
    printf("Sum is %d",sum);
    return 0;
}
